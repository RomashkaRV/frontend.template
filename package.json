{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3001 --turbopack",
    "build": "next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "quality": "npm run lint:ts && npm run lint:scss",
    "quality:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts": "next lint",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "^1.85.0"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@next/eslint-plugin-next": "^15.5.2",
    "@types/node": "22.7.5",
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.2",
    "eslint": "9.34.0",
    "lint-staged": "15.3.0",
    "react-html-props": "2.0.3",
    "typescript": "5.6.3",
    "string-replace-loader": "3.1.0",
    "@romashka_rv/frontend-config": "^2.6.0"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "commitlint": {
    "extends": "./node_modules/@romashka_rv/frontend-config/commitlintrc.json"
  },
  "eslintConfig": {
    "extends": "./node_modules/@romashka_rv/frontend-config/eslint.config.js"
  },
  "stylelint": {
    "extends": "./node_modules/@romashka_rv/frontend-config/stylelintrc.json"
  },
  "prettier": {
    "extends": "./node_modules/@romashka_rv/frontend-config/prettier.json"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "npm run lint:ts:fix",
    "**/*.{scss}": "npm run lint:scss:fix"
  }
}
